@model IEnumerable<gestion_de_consulta.Models.ConsultasGenerales>

<div>
    <div class="table-responsive">
        <table class="table table-hover" id="dataTable" width="100%" cellspacing="0">
            <thead>
                <tr>

                    <th>
                        @Html.DisplayNameFor(model => model.Identificacion)

                    </th>

                    <th>
                        @Html.DisplayNameFor(model => model.Nombres)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Apellidos)
                    </th>
                    <th>
                        Accion
                    </th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                <tr>

                    <td>
                        @Html.DisplayFor(modelItem => item.Identificacion)

                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Nombres)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Apellidos)

                    </td>
                    <td>
                        <a href="#" class="fa fa-vcard" style="color:blue"> Historial Clinico</a>
                    </td>
                </tr>
                }

            </tbody>
        </table>
    </div>
</div>
<script src="~/Scripts/jquery.dataTables.js"></script>
<script src="~/Scripts/dataTables.bootstrap4.js"></script>
<script src="~/Scripts/datatables-demo.js"></script>

